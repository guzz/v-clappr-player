{
  "name": "v-clappr-player",
  "version": "0.1.0",
  "private": true,
  "main": "dist/v-clappr-player.umd.js",
  "module": "dist/v-clappr-player.esm.js",
  "unpkg": "dist/v-clappr-player.min.js",
  "browser": {
    "./sfc": "src/v-clappr-player.vue"
  },
  "scripts": {
    "build": "yarn run build:umd & yarn run build:es & yarn run build:unpkg",
    "build:umd": "rollup --config rollup.config.js --format umd --file dist/v-clappr-player.umd.js",
    "build:es": "rollup --config rollup.config.js --format es --file dist/v-clappr-player.esm.js",
    "build:unpkg": "rollup --config rollup.config.js --format iife --file dist/v-clappr-player.min.js",
    "storybook": "start-storybook -s public -p 6006 --no-manager-cache",
    "build-docs": "build-storybook --docs -s public",
    "start-docs": "start-storybook --docs -s public --no-manager-cache"
  },
  "dependencies": {
    "@clappr/core": "^0.4.18",
    "@clappr/player": "^0.4.4",
    "@clappr/plugins": "^0.4.11",
    "@egoist/vue-to-react": "^1.1.0",
    "@guzzj/clappr-nerd-stats": "^2.0.4",
    "@guzzj/clappr-pip-plugin": "^2.0.2",
    "@guzzj/clappr-stats-plugin": "^0.2.0",
    "@guzzj/clappr-thumbnails-plugin": "^3.7.3",
    "@guzzj/clappr-youtube-playback": "^0.0.10",
    "@guzzj/dash-shaka-playback": "^3.0.1",
    "@guzzj/level-selector": "^0.2.1",
    "@storybook/theming": "^6.2.9",
    "clappr-context-menu-plugin": "^0.0.8",
    "clappr-queue-plugin": "^1.1.0",
    "core-js": "^3.6.5",
    "vue": "^2.6.12"
  },
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@rollup/plugin-buble": "^0.21.3",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/vue": "^6.2.9",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "markdown-to-jsx": "^7.1.2",
    "postcss": "^8.2.14",
    "rollup": "^1.17.0",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-vue": "^5.0.1",
    "ts-dedent": "^2.1.1",
    "vue": "^2.6.10",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
